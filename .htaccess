
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /current/all-microsite/git-templates/wp-template/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /current/all-microsite/git-templates/wp-template/index.php [L]
</IfModule>

# END WordPress